{
  "name": "appshot-2019",
  "version": "0.1.0",
  "description": "Personal Boilerplate for all the stuff I do",
  "main": "index.js",
  "author": "Ben Junya <bjunya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node ./build.js",
    "ci": "flow && jest",
    "coverage": "open ./coverage/lcov-report/index.html",
    "dev": "node ./build.js && webpack-dev-server",
    "flow": "flow",
    "flow-watch": "flow-watch",
    "generate": "hygen",
    "lint": "eslint \"src/**/*.js\"",
    "prettier": "prettier --write \"src/**/*.js\"",
    "start": "npm run dev",
    "styleguide": "styleguidist server",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@emotion/babel-preset-css-prop": "^10.0.7",
    "@emotion/core": "^10.0.7",

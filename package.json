    "babel-plugin-emotion": "^10.0.7",
    "classnames": "^2.2.6",
    "core-js": "^2.6.4",
    "dotenv": "^6.2.0",
    "history": "^4.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
